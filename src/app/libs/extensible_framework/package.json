{
  "name": "ext-framework-server",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "tsc -w",
    "test": "nyc mocha -r ts-node/register ./**/*.spec.ts --reporter mochawesome",
    "build": "rm -rf ./dist && tsc --p ./tsconfig.prod.json && cp package.json ./dist",
    "lint": "tslint --config ./tslint.json **/*.ts --force",
    "gen-doc": "typedoc --out ./docs ./src --name Extensible-framework --mode file --module commonjs --target ES6"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "**/*.js"
    ],
    "include": [
      "./src/**/*.ts"
    ],
    "reporter": [
      "html",
      "text-summary"
    ],
    "all": true
  },
  "keywords": [],
  "author": "Santhosh Vasabhaktula <santhosh@ilimi.in>",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/continuation-local-storage": "^3.2.1",
    "@types/elasticsearch": "^5.0.19",
    "@types/express": "^4.11.1",
    "@types/glob": "^5.0.35",
    "@types/lodash": "^4.14.104",
    "@types/mocha": "^2.2.48",
    "@types/sinon": "^4.1.3",
    "chai": "^4.1.2",
    "mocha": "^5.0.1",
    "mochawesome": "^3.0.2",
    "node-mocks-http": "^1.7.0",
    "nyc": "^11.8.0",
    "sinon": "^4.3.0",
    "ts-node": "^4.1.0",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.13.0",
    "tslint-config-standard": "^7.0.0",
    "typescript": "^2.8.4",
    "typedoc": "^0.11.1"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "continuation-local-storage": "^3.2.1",
    "elasticsearch": "^14.1.0",
    "express": "^4.16.2",
    "express-cassandra": "^2.2.2",
    "glob": "^7.1.2",
    "hashids": "^1.1.4",
    "keycloak-connect": "^4.0.0-beta.2",
    "lodash": "^4.17.5",
    "log4js": "^2.10.0",
    "rxjs": "^6.2.1",
    "typescript-ioc": "^1.2.4",
    "uuid": "^3.2.1"
  }
}
