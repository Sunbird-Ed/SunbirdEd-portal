{
  "name": "player",
  "author": "sunbird",
  "version": "4.2.0",
  "buildNumber": "1",
  "private": true,
  "devDependencies": {
    "babel": "6.23.0",
    "chai": "4.2.0",
    "chai-http": "4.2.0",
    "chai-spies": "1.0.0",
    "codacy-coverage": "3.0.0",
    "gulp": "4.0.2",
    "gulp-rename": "2.0.0",
    "mocha": "4.1.0",
    "mochawesome": "2.3.1",
    "mock-require": "3.0.3",
    "node-mocks-http": "1.8.1",
    "nyc": "14.1.1",
    "path": "0.12.7",
    "sinon": "4.5.0",
    "webpack-bundle-analyzer": "^3.7.0"
  },
  "engines": {
    "node": ">=12.16.1",
    "npm": ">=6.13.4"
  },
  "nodemonConfig": {
    "ignore": [
      "client"
    ],
    "exec": "npm run server",
    "ext": "png,jpeg,js,ts,html,json,css,scss"
  },
  "scripts": {
    "server": "gulp update:index:file && node server",
    "validate": "npm ls",
    "precommit": "lint-staged",
    "resource-bundles": "node helpers/resourceBundles/build.js",
    "test-with-coverage": "cd client && npm run test-coverage && cat ./coverage/lcov.info | codacy-coverage",
    "tenants-push-cdn": "gulp pushTenantsToCDN --gulpfile gulp-tenant.js",
    "bundle-report": "webpack-bundle-analyzer dist/stats.json --port 8887",
    "backend-test": "mocha 'tests/server/**/*.spec.js'",
    "latex-test": "mocha 'tests/server/helpers/spec/latexService.spec.js'",
    "backend-test-with-coverage": "nyc --reporter=html --reporter=lcov npm run backend-test"
  },
  "nyc": {
    "all": true,
    "include": [
      "routes/*.js",
      "proxy/*.js",
      "helpers/*.js"
    ],
    "extension": [
      ".js"
    ],
    "exclude": [
      "helpers/resourceBundles/*.js"
    ]
  },
  "dependencies": {
    "@project-sunbird/discussion-service": "0.0.6",
    "@project-sunbird/ext-framework-server": "0.1.28",
    "@project-sunbird/form-service": "0.0.13",
    "@project-sunbird/logger": "^0.0.9",
    "@project-sunbird/review-comment": "0.0.8",
    "async": "2.5.0",
    "axios": "^0.20.0",
    "azure-storage": "2.10.2",
    "body-parser": "1.17.2",
    "cassandra-session-store": "1.0.6",
    "compare-versions": "3.5.1",
    "compression": "1.7.2",
    "connect-redis": "4.0.4",
    "cookie-parser": "1.4.4",
    "dateformat": "2.0.0",
    "ejs": "2.5.6",
    "express": "4.15.3",
    "express-cassandra": "2.1.0",
    "express-http-proxy": "1.0.6",
    "express-session": "1.15.3",
    "google-auth-library": "^7.1.0",
    "googleapis": "45.0.0",
    "helmet": "3.21.2",
    "http-graceful-shutdown": "^2.3.2",
    "http-status-codes": "1.3.0",
    "jsonwebtoken": "7.4.1",
    "kafka-node": "3.0.1",
    "keycloak-connect": "git+https://github.com/project-sunbird/keycloak-nodejs-connect.git#release-2.5.0",
    "lodash": "4.17.15",
    "mathjax-full": "3.0.0",
    "mobile-detect": "1.4.2",
    "morgan": "^1.10.0",
    "multiparty": "^4.2.1",
    "node-rsa": "^1.1.1",
    "path-to-regexp": "^6.1.0",
    "properties": "1.2.1",
    "redis": "3.0.2",
    "request": "2.81.0",
    "request-promise": "^4.2.5",
    "sb_api_interceptor": "file:libs/sb_api_interceptor",
    "sb_cache_manager": "file:libs/sb_cache_manager",
    "sb_telemetry_util": "file:libs/sb_telemetry_util",
    "sunbird-localization": "0.0.2",
    "svg2img": "0.6.1",
    "ua-parser-js": "0.7.13",
    "uuid": "3.1.0",
    "uuid-validate": "0.0.3"
  },
  "standard": {
    "env": {
      "browser": true,
      "node": true,
      "jasmine": true,
      "mocha": true
    },
    "globals": [
      "angular",
      "_",
      "$",
      "moment",
      "iziToast",
      "org",
      "alasql",
      "md5",
      "EkTelemetry"
    ],
    "ignore": [
      "./../Dockerfile"
    ],
    "rules": {
      "indent": [
        "error",
        2
      ],
      "max-len": [
        "error",
        120,
        2,
        {
          "ignoreUrls": true,
          "ignoreComments": true
        }
      ],
      "quotes": [
        "error",
        "single"
      ]
    }
  }
}
