{
  "name": "player",
  "author": "sunbird",
  "version": "2.5.0",
  "buildNumber": "1",
  "private": true,
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-http": "^4.2.0",
    "eslint": "^4.3.0",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jasmine": "2.9.1",
    "eslint-plugin-mocha": "4.11.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "gulp": "^3.9.1",
    "gulp-clean": "0.4.0",
    "gulp-decompress": "2.0.2",
    "gulp-deploy-azure-cdn": "^2.1.0",
    "gulp-download": "0.0.1",
    "gulp-gzip": "1.4.2",
    "gulp-rename": "1.2.2",
    "gulp-rev": "^8.1.1",
    "gulp-rev-collector": "^1.3.1",
    "gulp-rev-del-redundant": "^1.1.5",
    "gulp-rimraf": "0.2.2",
    "gulp-sequence": "1.0.0",
    "gulp-string-replace": "1.1.1",
    "gulp-url-prefixer": "0.0.6",
    "mocha": "^4.0.1",
    "mochawesome": "^2.3.1",
    "path": "^0.12.7",
    "phantomjs-prebuilt": "2.1.15",
    "require-dir": "0.3.2",
    "require-dot-file": "^0.4.0",
    "rev-del": "^1.0.5",
    "rimraf": "^2.4.0",
    "rmdir": "^1.2.0",
    "run-sequence": "2.2.1",
    "save": "2.3.2",
    "sinon": "^4.0.1",
    "yargs": "^9.0.1"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "nodemonConfig": {
    "ignore": [
      "client"
    ],
    "exec": "npm run server",
    "ext": "png,jpeg,js,ts,html,json,css,scss"
  },
  "scripts": {
    "server": "gulp update:index:file && node server",
    "test-announcements": "mocha 'test/announcements/server/**/*.js' --reporter mochawesome",
    "preinstall": "npm install ./libs/sb_api_interceptor && npm install ./libs/sb_cache_manager  && npm install ./libs/sb_telemetry_util && npm install ./libs/sb_logger_util_v2",
    "validate": "npm ls",
    "precommit": "lint-staged",
    "resource-bundles": "node helpers/resourceBundles/build.js",
    "deploy": "gulp deploy",
    "test-with-coverage": "cd client && npm run test-coverage && cat ./coverage/lcov.info | codacy-coverage",
    "tenants-push-cdn": "gulp pushTenantsToCDN --gulpfile gulp-tenant.js",
    "bundle-report": "webpack-bundle-analyzer dist/stats.json --port 8887"
  },
  "dependencies": {
    "@project-sunbird/discussion-service": "0.0.6",
    "@project-sunbird/ext-framework-server": "0.1.10",
    "@project-sunbird/form-service": "0.0.12",
    "@project-sunbird/review-comment": "0.0.8",
    "async": "^2.5.0",
    "asyncawait": "^1.0.7",
    "azure-storage": "^2.10.2",
    "body-parser": "^1.17.2",
    "cassandra-session-store": "1.0.6",
    "codacy-coverage": "3.0.0",
    "compare-versions": "^3.5.1",
    "compression": "1.7.2",
    "cookie-parser": "^1.4.4",
    "dateformat": "^2.0.0",
    "ejs": "^2.5.6",
    "express": "^4.15.3",
    "express-cassandra": "2.1.0",
    "express-http-proxy": "^1.0.6",
    "express-session": "^1.15.3",
    "googleapis": "^36.0.0",
    "gulp-brotli": "^1.2.1",
    "helmet": "3.8.1",
    "http-status-codes": "^1.3.0",
    "joi": "^13.0.1",
    "js-md5": "^0.5.0",
    "jsonwebtoken": "^7.4.1",
    "kafka-node": "^4.1.1",
    "keycloak-connect": "git+https://github.com/project-sunbird/keycloak-nodejs-connect.git#release-2.3.5",
    "lodash": "^4.17.4",
    "mobile-detect": "^1.4.2",
    "multer": "^1.3.0",
    "node-json-transform": "^1.0.13",
    "opencollective-postinstall": "^2.0.0",
    "properties": "^1.2.1",
    "request": "^2.81.0",
    "request-promise-native": "^1.0.5",
    "sb_api_interceptor": "file:libs/sb_api_interceptor",
    "sb_cache_manager": "file:libs/sb_cache_manager",
    "sb_logger_util_v2": "file:libs/sb_logger_util_v2",
    "sb_telemetry_util": "file:libs/sb_telemetry_util",
    "ua-parser-js": "^0.7.13",
    "uuid": "^3.1.0"
  },
  "standard": {
    "env": {
      "browser": true,
      "node": true,
      "jasmine": true,
      "mocha": true
    },
    "globals": [
      "angular",
      "_",
      "$",
      "moment",
      "iziToast",
      "org",
      "alasql",
      "md5",
      "EkTelemetry"
    ],
    "ignore": [
      "./../Dockerfile"
    ],
    "rules": {
      "indent": [
        "error",
        2
      ],
      "max-len": [
        "error",
        120,
        2,
        {
          "ignoreUrls": true,
          "ignoreComments": true
        }
      ],
      "quotes": [
        "error",
        "single"
      ]
    }
  }
}
