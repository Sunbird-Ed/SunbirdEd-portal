<!doctype html>
<html ng-app="loginApp">

<head>
    <meta charset="utf-8">
    <title ng-bind="$root.titleName"></title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <link rel="icon" type="image/ico" ng-href="{{faviconIcon || '/common/images/favicon.ico'}}">
    <link rel="stylesheet" href="/themes/<%= theme ? theme : 'default' %>/semantic.min.css">

    <!-- inject:css -->
    <link rel="stylesheet" href="/thirdparty/bower_components/semantic-ui-calendar/dist/calendar.min.css" />
    <link rel="stylesheet" href="/thirdparty/bower_components/izitoast/dist/css/iziToast.min.css" />
    <!-- endinject -->
    <link rel="stylesheet" href="/thirdparty/bower_components/jquery.fancytree/dist/skin-win8/ui.fancytree.min.css">
    <link rel="stylesheet" href="/themes/<%= theme ? theme : 'default' %>/assets/styles/main.css">
    <base href="/">
</head>

<body ng-controller="loginCtrl">
    <input type="hidden" id="cdnUrl" value="<%=cdnUrl%>" />
    <input type="hidden" id="defaultPortalLanguage" value="<%=defaultPortalLanguage%>" />
    <div ng-include="'views/header/publicHeader.html'"></div>
    <div class="ui top attached fluid container">
        <div class="ui padded grid">
            <div class="row">
                <div class="twelve wide column">
                    <div ui-view="mainView">
                    </div>
                </div>
            </div>
        </div>
    </div>
    <script src="../common/js/org.js"></script>
    <!-- inject:js -->
    <script src="/thirdparty/libs/eventbus.min.js"></script>
    <script src="/thirdparty/bower_components/jquery/dist/jquery.min.js"></script>
    <script src="/thirdparty/bower_components/jquery-ui/jquery-ui.min.js"></script>
    <script src="/thirdparty/bower_components/angular/angular.min.js"></script>
    <script src="/thirdparty/bower_components/angular-cookies/angular-cookies.min.js"></script>
    <script src="/thirdparty/bower_components/angular-route/angular-route.min.js"></script>
    <script src="/thirdparty/bower_components/angular-ui-router/release/angular-ui-router.min.js"></script>
    <script src="/thirdparty/bower_components/semantic-ui-calendar/dist/calendar.min.js"></script>
    <script src="/thirdparty/bower_components/izitoast/dist/js/iziToast.min.js"></script>
    <script src="/thirdparty/bower_components/angular-uuid4/angular-uuid4.min.js"></script>
    <script src="/thirdparty/bower_components/lodash/lodash.js"></script>
    <script src="/thirdparty/bower_components/moment/min/moment-with-locales.min.js"></script>
    <script src="/thirdparty/bower_components/jquery.fancytree/dist/jquery.fancytree.min.js"></script>
    <script src="/thirdparty/bower_components/angular-sanitize/angular-sanitize.min.js"></script>

    <script src="/thirdparty/semantic/semantic.min.js"></script>
    <script src="/private/scripts/utils/util.js"></script>
    <script src="/private/scripts/managers/eventManager.js"></script>
    <script src="/public/scripts/publicAppConfig.js"></script>
    <script src="/public/scripts/application.js"></script>
    <script src="/public/scripts/routes/publicAppRoute.js"></script>
    <script src="/public/scripts/service/httpService.js"></script>
    <script src="/public/scripts/service/signUpService.js"></script>
    <script src="/public/scripts/service/toasterService.js"></script>
    <script src="/public/scripts/service/contentService.js"></script>
    <script src="/public/scripts/directive/error.js"></script>
    <script src="/public/scripts/directive/loader.js"></script>
    <script src="/public/scripts/directive/courseDiscussions.js"></script>
    <script src="/public/scripts/directive/publicContentPlayer.js"></script>
    <script src="/public/scripts/controllers/signUpController.js"></script>
    <script src="/public/scripts/controllers/publicContentPlayerController.js"></script>
    <script src="/public/scripts/controllers/courseScheduleController.js"></script>
    <script src="/public/scripts/controllers/collectionPlayerController.js"></script>
    <script src="/public/scripts/filters/dateFilter.js"></script>
    <!-- endinject -->
    <script src="../common/js/frmelmnts.js"></script>
    <script src="../common/js/messages.js"></script>
</body>

</html>