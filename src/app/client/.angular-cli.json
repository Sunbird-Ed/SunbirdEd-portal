{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "src"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "../dist",
      "assets": [
        "assets",
        "thirdparty",
        "favicon.png"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "./assets/styles/semantic.min.css",
        "styles.css",
        "../node_modules/font-awesome/css/font-awesome.css",
        "../node_modules/izitoast/dist/css/iziToast.min.css",
        "../node_modules/fine-uploader/fine-uploader/fine-uploader-gallery.min.css",
        "./assets/libs/semantic-ui-tree-picker/semantic-ui-tree-picker.css",
        "../node_modules/jquery.fancytree/dist/skin-awesome/ui.fancytree.css"
      ],
      "scripts": [
        "../node_modules/moment/min/moment.min.js",
        "../node_modules/izitoast/dist/js/iziToast.min.js",
        "../node_modules/jquery/dist/jquery.min.js",
        "../node_modules/slick-carousel/slick/slick.js",
        "assets/libs/pagedown-core/Markdown.Converter.js",
        "assets/libs/pagedown-core/Markdown.Editor.js",
        "assets/libs/pagedown-core/Markdown.Sanitizer.js",
        "assets/libs/telemetry.min.js",
        "../node_modules/fine-uploader/fine-uploader/fine-uploader.min.js",
        "assets/libs/semantic.min.js",
        "assets/libs/semantic-ui-tree-picker/semantic-ui-tree-picker.js",
        "../node_modules/tree-model/dist/TreeModel-min.js",
        "../node_modules/jquery.fancytree/dist/jquery.fancytree-all-deps.min.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts",
        "staging": "environments/environment.staging.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "src/tsconfig.spec.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "e2e/tsconfig.e2e.json",
      "exclude": "**/node_modules/**"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "component": {}
  }
}